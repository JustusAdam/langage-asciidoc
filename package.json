{
	"name": "Asciidoc",
	"version": "0.0.4",
	"publisher": "amoiseev",
	"description": "Asciidoc language support (with Asciidoctor flavor).",
	"icon": "images/asciidoctor.png",
	"repository": {
		"type": "git",
		"url": "https://github.com/antonmoiseev/vscode-asciidoc.git"
	},
	"engines": {
		"vscode": "0.10.x"
	},
	"categories": [
		"Languages"
	],
	"galleryBanner": {
		"color": "#136E9E",
		"theme": "dark"
	},
	"contributes": {
		"languages": [
			{
				"id": "asciidoc",
				"aliases": ["Asciidoc[tor]", "asciidoc"],
				"extensions": [".adoc",".ad",".asciidoc"],
				"configuration": "./asciidoc.configuration.json"
			}
		],
		"grammars": [
			{
				"language": "asciidoc",
				"scopeName": "text.asciidoc",
				"path": "./syntaxes/asciidoc.json"
			}
		]
	}
}
